
public class CourseNode {

	String id;
	String firstName;
	String surname;
	String graduateDepartmant;
	CourseNode next;
	public CourseNode(String id, String firstName, String surname, String graduateDepartmant) {
		this.id = id;
		this.firstName = firstName;
		this.surname = surname;
		this.graduateDepartmant = graduateDepartmant;
		this.next = null;
	}
	
	
}
