
public class Main {

	public static void main(String[] args) {
		NodeClass A=new ExClass();   //We created the nodes in here.
		NodeClass B=new ExClass();
		NodeClass C=new ExClass();
				
		A.number=11;
		B.number=12;
		C.number=13;
		
		A.next=B;  //We tied the nodes together in here.
		B.next=C;
		C.next=null;
		
		NodeClass temp=A;
		while(temp.next!=null)
		{
			System.out.print.ln(temp.number);
			temp=temp.next;
		}
	}

}
